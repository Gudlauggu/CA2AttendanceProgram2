<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="345.0" prefWidth="422.0" stylesheets="@MainLayout.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca1attendanceprogram.GUI.Controller.ChangePasswordViewController">
   <children>
      <Label layoutX="14.0" layoutY="34.0" prefHeight="26.0" prefWidth="182.0" styleClass="label-headline" text="Change Password" textFill="WHITE" />
      <Label layoutX="36.0" layoutY="221.0" />
      <Button fx:id="btnCancel" layoutX="218.0" layoutY="295.0" mnemonicParsing="false" onAction="#cancelEvent" text="Cancel" />
      <Button fx:id="btnChange" layoutX="305.0" layoutY="295.0" mnemonicParsing="false" onAction="#changePass" text="Change" />
      <VBox layoutX="6.0" layoutY="59.0" prefHeight="224.0" prefWidth="408.0" spacing="30.0">
         <children>
            <TextField fx:id="txtUsername" promptText="Username" />
            <PasswordField fx:id="txtNewPass" promptText="New Password" />
            <PasswordField fx:id="txtOldPass" promptText="Old Password" />
            <PasswordField fx:id="txtNewPassAgain" promptText="New Password Again" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
